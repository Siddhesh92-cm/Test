def build_path(df):
    stack = {}
    paths = []

    for _, row in df.iterrows():
        level = row["Level"]
        task = row["Task"]

        stack[level] = task

        # remove deeper levels
        stack = {k: v for k, v in stack.items() if k <= level}

        full_path = " > ".join([stack[l] for l in sorted(stack)])
        paths.append(full_path)

    return paths
