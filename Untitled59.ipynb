def build_path_map(node, path="", path_map=None):
    if path_map is None:
        path_map = {}

    if node.name != "ROOT":
        current_path = f"{path}/{node.name}" if path else node.name
        path_map[current_path] = node
    else:
        current_path = ""

    for child in node.children:
        build_path_map(child, current_path, path_map)

    return path_map
