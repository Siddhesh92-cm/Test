def regenerate_wbs(df):
    counters = {}
    wbs = []

    for _, row in df.iterrows():
        level = row["Level"]

        counters[level] = counters.get(level, 0) + 1

        # reset deeper levels
        for k in list(counters.keys()):
            if k > level:
                counters[k] = 0

        current = ".".join(str(counters[l]) for l in sorted(counters) if l <= level and counters[l] > 0)
        wbs.append(current)

    return wbs

df_final["WBS"] = regenerate_wbs(df_final)
