def assign_level_and_wbs(node, level=0, prefix=[]):
    if node.name != "ROOT":
        node.level = level
        node.wbs = ".".join(map(str, prefix))

    for i, child in enumerate(node.children, 1):
        assign_level_and_wbs(child, level + 1, prefix + [i])
