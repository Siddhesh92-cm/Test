def merge_trees(template_root, mpp_root):
    template_map = build_path_map(template_root)
    mpp_map = build_path_map(mpp_root)

    for path, mpp_node in mpp_map.items():
        if path not in template_map:
            parent_path = "/".join(path.split("/")[:-1])
            node_name = path.split("/")[-1]

            parent_node = template_map.get(parent_path)

            if parent_node:
                new_node = TaskNode(node_name)
                parent_node.add_child(new_node)

                # update map
                template_map[path] = new_node

    return template_root
