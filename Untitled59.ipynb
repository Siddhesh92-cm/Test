def flatten_tree(node, rows=None):
    if rows is None:
        rows = []

    if node.name != "ROOT":
        rows.append({
            "Task": node.name,
            "Level": node.level,
            "WBS": node.wbs
        })

    for child in node.children:
        flatten_tree(child, rows)

    return rows
